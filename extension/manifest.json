{
  "manifest_version": 3,
  "name": "Web3 小狗助手",
  "version": "1.0",
  "description": "你的 Web3 风险提醒桌宠",
  "permissions": ["activeTab", "storage"],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["lottie.min.js", "content.js"],
      "css": ["style.css"]
    }
  ],
"web_accessible_resources": [
    {
      "resources": ["Doge.json", "images/GoodHunter.png"],
      "matches": ["<all_urls>"]
    }
  ]
  
}